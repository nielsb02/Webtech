    "version": "0.2.0",
    "configurations": [
        {
            "type": "firefox",
            "request": "launch",
            "name": "Launch Firefox",
            "file": "${workspaceFolder}/Webtech/index.html"
        },
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Launch Chrome",
            "file": "${workspaceFolder}/Webtech/index.html"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Webtech application",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/webtechapp/main.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Rendered express",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/webtechApplication/webtechApplication/app.js"
        }
    ]
}